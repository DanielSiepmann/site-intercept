{
  "name": "t3g/intercept",
  "description": "",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Susanne Moog",
      "email": "susanne.moog@typo3.com"
    }
  ],
  "require": {
    "ext-SimpleXML": "*",
    "ext-bcmath": "*",
    "ext-json": "*",
    "cpliakas/git-wrapper": "^2.1",
    "eightpoints/guzzle-bundle": "^7.4",
    "graylog2/gelf-php": "^1.6",
    "kbsali/redmine-api": "~1.0",
    "php-amqplib/php-amqplib": "^2.8",
    "psr/http-message": "^1",
    "psr/log": "^1",
    "ramsey/uuid": "^3.8",
    "sensio/framework-extra-bundle": "^5.2",
    "symfony/http-foundation": "^4.1",
    "symfony/console": "^4.1",
    "symfony/dependency-injection": "^4.1",
    "symfony/dotenv": "^4.1",
    "symfony/filesystem": "^4.1",
    "symfony/flex": "^1.1",
    "symfony/form": "^4.1",
    "symfony/framework-bundle": "^4.1",
    "symfony/ldap": "^4.1",
    "symfony/monolog-bundle": "^3.3",
    "symfony/orm-pack": "^1.0",
    "symfony/routing": "^4.1",
    "symfony/security-bundle": "^4.1",
    "symfony/security-csrf": "^4.1",
    "symfony/security-core": "^4.1",
    "symfony/serializer": "^4.1",
    "symfony/twig-bundle": "^4.1",
    "symfony/validator": "^4.1",
    "symfony/webpack-encore-bundle": "^1.0",
    "symfony/yaml": "^4.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.12",
    "phpunit/phpunit": "^7.4",
    "roave/security-advisories": "dev-master",
    "symfony/browser-kit": "^4.1",
    "symfony/css-selector": "^4.1",
    "symfony/maker-bundle": "^1.10",
    "symfony/profiler-pack": "^1.0",
    "symfony/var-dumper": "^4.1"
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.2.4"
    },
    "preferred-install": {
      "*": "dist"
    }
  },
  "scripts": {
    "t3g:test:php:unit": [
      "mkdir -p var/phpunit/; phpunit -c build/phpunit.xml --log-junit var/phpunit/phpunit.xml  --coverage-clover var/phpunit/coverage.xml --coverage-html var/phpunit/coverage/"
    ],
    "t3g:test": [
      "@t3g:test:php:unit"
    ],
    "t3g:cgl": [
      "mkdir -p var/php-cs-fixer/; php-cs-fixer fix --config build/.php_cs.dist --format=junit > var/php-cs-fixer/php-cs-fixer.xml"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "extra": {
    "symfony": {
      "allow-contrib": false
    }
  }
}
